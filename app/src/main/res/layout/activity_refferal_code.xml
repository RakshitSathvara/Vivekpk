<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="in.vaksys.vivekpk.activities.RefferalCodeActivity">

    <include
        android:id="@+id/refferalToolbar"
        layout="@layout/toolbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#ECE9E8"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtZero"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/arrowOne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_action_back_arrow" />

        <TextView
            android:id="@+id/pos_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:text="Help Center"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/arrowTwo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_action_back_arrow" />

        <TextView
            android:id="@+id/pos_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:text="Help Center"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>


    <include
        android:id="@+id/helpCenter"
        layout="@layout/help_center" />

    <include
        android:id="@+id/myAccountView"
        layout="@layout/help" />

    <include
        android:id="@+id/accountClick"
        layout="@layout/help_my_account" />

    <include
        android:id="@+id/help_register_vehicle"
        layout="@layout/help_register_vehical" />

    <include
        android:id="@+id/help_emergency_contact"
        layout="@layout/help_emergency_contact" />

    <include
        android:id="@+id/help_set_reminder"
        layout="@layout/help_set_reminder" />

    <include
        android:id="@+id/help_document_list"
        layout="@layout/help_document_list" />

    <include
        android:id="@+id/troubleshooting"
        layout="@layout/troubleshooting" />

    <include
        android:id="@+id/insurance_reminder"
        layout="@layout/insurance_reminder" />

    <include
        android:id="@+id/emission_reminder"
        layout="@layout/emssion_reminder" />

    <include
        android:id="@+id/service_reminder"
        layout="@layout/service_reminder" />

    <include
        android:id="@+id/driving_License"
        layout="@layout/driving_license_list_one" />

    <include
        android:id="@+id/document_list_insurance"
        layout="@layout/document_list_insurance" />

    <include
        android:id="@+id/document_list_emission"
        layout="@layout/document_list_emission" />

    <include
        android:id="@+id/document_list_bills"
        layout="@layout/document_list_bills" />

</LinearLayout>
